@model WebUI.Models.SubscribersView

@{
    ViewBag.Title = "Управление подписчиками";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-lg-6">
    <h1 class="span-title">Управление подписчиками</h1>
</div>
<div class="col-lg-6">
    <div class="pagination pull-right" style="margin-top:5px;">
        @{
            @Html.PageLinks(Model.PagingInfo, x => Url.Action("managesubscribers", new { page = x }))
        }
    </div>
</div>

<div class="col-lg-12">
    @Html.Action("SubscribersList", "home")
</div>

<div class="col-lg-offset-6 col-lg-6">
    <div class="pagination pull-right" style="margin-top:0;">
        @{
            @Html.PageLinks(Model.PagingInfo, x => Url.Action("managesubscribers", new { page = x }))
        }
    </div>
</div>